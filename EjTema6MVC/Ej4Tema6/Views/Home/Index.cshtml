@{
    ViewData["Title"] = "Home Page";
}

<style>
table, th, td
{
        border: 3px solid black;
}
</style>
<table>

@for (int i=1; i <=12; i++){
 
        
    <tr>
    @for (int j=1; j <=10; j++) {
        int@c = @i * @j;
            
                <td> @j * @i = @c </td>
    }
    </tr>
       
}
</table>
 
